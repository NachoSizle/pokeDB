# Configuración para resolver problemas con dependencias opcionales de Rollup
optional=true
install-optional=true
save-optional=true

# Forzar instalación de dependencias nativas de Rollup
@rollup:registry=https://registry.npmjs.org/

# Configuración específica para Vercel
prefer-offline=false
cache-max=0
fund=false
audit=false
ignore-scripts=false

# Resolver problemas específicos de arquitectura
target_arch=x64
target_platform=linux
